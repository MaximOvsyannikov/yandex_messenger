<div class="profileField__root {{#if underline}} profileField__underline {{/if}}">
  <span class="profileField__label">{{label}}</span>
  <input
    type="{{type}}"
    class="profileField__input"
    name="{{name}}"
    value="{{value}}"
    placeholder="{{placeholder}}"
    onchange={{onchange}}
  />
</div>